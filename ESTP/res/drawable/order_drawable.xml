<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.1dp" android:color="#84909c"></stroke>
            <corners android:radius="15dp"></corners>
        </shape>
    </item>

    <item>
        <shape>
            <stroke android:width="0.1dp" android:color="@color/toolbar_bg_color"></stroke>
            <corners android:radius="15dp"></corners>
        </shape>
    </item>
</selector>